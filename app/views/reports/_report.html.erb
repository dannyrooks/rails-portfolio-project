
<h5><%= report.title %> </h5><br>
<h6>Report: <br> <%= report.content %></h6><br>
<p>Posted by <%= report.user.email %><br>

<%= time_ago_in_words(report.created_at) %> ago</p>

<% if user_signed_in? %>
    <% if report.user_id == current_user.id %>
    <%= link_to 'Edit', edit_location_report_path(report.location, report) %> |
    <%= link_to 'Delete', location_report_path(report.location, report), method: :delete, data: { confirm: "Are you sure you want to delete?" } %>
    <% end %>
<% end %>  
<br>  
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
